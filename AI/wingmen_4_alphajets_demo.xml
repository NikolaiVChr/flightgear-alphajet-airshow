			<parent type="string"></parent>
			<pitch-offset>0</pitch-offset>
			<roll-offset>0</roll-offset>
			<yaw-offset>0</yaw-offset>
			<ground-offset>0</ground-offset>
			<coefficients>
				<heading>5.0</heading>
				<pitch>4.0</pitch>
				<bank>4.0</bank>
				<speed>9.9</speed>
			</coefficients>
