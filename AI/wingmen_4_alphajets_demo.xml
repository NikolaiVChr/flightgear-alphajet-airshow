			<y-offset>-63</y-offset>
			<z-offset>0</z-offset>
			<formate>true</formate>
			<max-speed-kts>540</max-speed-kts>
			<azimuth>0</azimuth>
			<elevation>0</elevation>
			<life>-1</life>
			<no-roll type="bool">false</no-roll>
